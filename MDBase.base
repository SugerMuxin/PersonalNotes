formulas:
  Untitled: file.embeds.filter(value.containsAny("png","jpg","webp","svg","jpeg"))[0]
  Untitled 2: ""
properties:
  formula.Untitled:
    displayName: coverimg
views:
  - type: cards
    name: Table
    filters:
      and:
        - file.ext != "png"
        - file.ext != "pdf"
        - file.ext != "rar"
        - file.ext != "exe"
        - file.ext != "bat"
        - file.ext != "base"
        - file.ext != "canvas-llm"
        - file.folder != "Pics"
        - file.folder != "Excalidraw"
        - file.folder != "Books"
        - file.name.contains("Claude")
    groupBy:
      property: file.folder
      direction: ASC
    order:
      - tags
      - author
      - file.name
    columnSize:
      note.tags: 345
    imageAspectRatio: 0.9
    cardSize: 200
    image: formula.Untitled
